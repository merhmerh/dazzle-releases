#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/@capacitor+cli@7.2.0/node_modules/@capacitor/cli/bin/node_modules:/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/@capacitor+cli@7.2.0/node_modules/@capacitor/cli/node_modules:/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/@capacitor+cli@7.2.0/node_modules/@capacitor/node_modules:/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/@capacitor+cli@7.2.0/node_modules:/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/@capacitor+cli@7.2.0/node_modules/@capacitor/cli/bin/node_modules:/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/@capacitor+cli@7.2.0/node_modules/@capacitor/cli/node_modules:/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/@capacitor+cli@7.2.0/node_modules/@capacitor/node_modules:/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/@capacitor+cli@7.2.0/node_modules:/home/merh/projects/dazzle/dazzle-releases/capacitor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/capacitor" "$@"
else
  exec node  "$basedir/../../bin/capacitor" "$@"
fi
